{
	"manifest_version": 3,
	"name": "rickrolln't",
	"author": "colenh",
	"short_name": "rickrolln't",
	"version": "1.1.2",
	"description": "redirects rickrolls and more to a diffferent page",
	"icons": {
		"512": "icons/rickrollnt.png"
	},
	"background": {
		"service_worker": "background.js"
	},
	"permissions": [
		"webRequest",
		"webRequestBlocking"
	],
	"host_permissions": [
		"*://youtube.com/*",
		"*://*.youtube.com/*",
		"*://*.thisworldthesedays.com/*",
		"*://*.noordstar.me/*",
		"*://rickrolled.com/"
	],
	"optional_host_permissions": [
		"*://*/*"
	],
	"browser_specific_settings": {
		"gecko": {
			"id": "{7e52e1e6-693b-4301-ae99-4c91bf2a1dd6}"
		}
	}
}
